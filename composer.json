{
    "name": "skovachev/fakefactory",
    "description": "A model factory package for Laravel 4 with expressive API for creating custom tailored dummy objects",
    "keywords": ["faker", "fixtures", "factory", "models", "laravel", "seed", "tool"],
    "authors": [
        {
            "name": "Stefan Kovachev",
            "email": "contact@stefankovachev.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "fzaninotto/faker": "1.3.*@dev"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "psr-0": {
            "Skovachev\\Fakefactory": "src/"
        },
        "classmap": [
            "tests/TestCase.php",
            "tests/DummyFakerClass.php"
        ]
    },
    "minimum-stability": "dev"
}